<div class="row">
	<div class="span10 offset1">

		<%= form_for(@core_termo_fiel_depositario, :html=>{:class=>"form-horizontal well"}) do |f| %>
		<% if @core_termo_fiel_depositario.errors.any? %>
		<div class="alert alert-error">
			<button type="button" class="close" data-dismiss="alert">&times;</button>
			<strong><%= pluralize(@core_termo_fiel_depositario.errors.count, "erro") %> ao tentar salvar este formulario:</strong>
			
			<ul>
				<% @core_termo_fiel_depositario.errors.full_messages.each do |msg| %>
				<li>
					<%= msg %>
				</li>
				<% end %>
			</ul>
		</div>
		<% end %>
				
		
		<div class="control-group">
			<%= f.label :rodoviario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :rodoviario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :rodoviario_usuario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :rodoviario_usuario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :rodoviario_data_autorizacao, :class=>"control-label" %>
			<div class="controls">
				<%= f.datetime_select :rodoviario_data_autorizacao, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :fisco_usuario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :fisco_usuario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :fisco_data_autorizacao, :class=>"control-label" %>
			<div class="controls">
				<%= f.datetime_select :fisco_data_autorizacao, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :porto_usuario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :porto_usuario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :porto_data_autorizacao, :class=>"control-label" %>
			<div class="controls">
				<%= f.datetime_select :porto_data_autorizacao, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :status_termo_fiel_depositario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :status_termo_fiel_depositario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :fisco_fechamento_usuario_id, :class=>"control-label" %>
			<div class="controls">
				<%= f.number_field :fisco_fechamento_usuario_id, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :fechamento_descricao, :class=>"control-label" %>
			<div class="controls">
				<%= f.text_area :fechamento_descricao, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="control-group">
			<%= f.label :fechamento_data, :class=>"control-label" %>
			<div class="controls">
				<%= f.datetime_select :fechamento_data, :class=>"input-xlarge" %>
				<p class="help-block">
					
				</p>
			</div>
		</div>
		
		<div class="form-actions">
			<%= f.submit :class=>"btn btn-primary" %>
			<button type="reset" class="btn">
				Cancelar
			</button>
		</div>
		<% end %>

	</div>
</div>
